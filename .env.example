# Configurações da API da Clínica nas Nuvens
CLINICA_NAS_NUVENS_API_URL=https://api.clinicanasnuvens.com.br
CLINICA_NAS_NUVENS_USERNAME=seu_usuario
CLINICA_NAS_NUVENS_PASSWORD=sua_senha
CLINICA_NAS_NUVENS_CID=seu_token_cid

# Configurações do banco de dados
DATABASE_CONNECTION_URI=postgresql://usuario:senha@host:5432/database_name

# Configuração de webhook
WEBHOOK_URL=https://seu-webhook-url.com/webhook

# Configuração de domínio
DOMAIN=seu-dominio.com

# Configuração RabbitMQ (opcional)
RABBITMQ_ENABLED=false
RABBITMQ_URI=amqp://usuario:senha@host:5672/vhost
